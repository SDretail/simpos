{
  "productName": "Simpos",
  "version": "0.0.0",
  "identifier": "com.fibotree.simpos",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Simpos",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "hiddenTitle": true,
        "minHeight": 700,
        "minWidth": 1000
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
